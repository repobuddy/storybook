{
	"name": "@repobuddy/storybook-vitesnap",
	"description": "Capture and compare image snapshot automatically and manually",
	"version": "0.0.0",
	"private": true,
	"keywords": [
		"storybook-addons"
	],
	"type": "module",
	"license": "MIT",
	"author": "unional <homawong@gmail.com>",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./preview": {
			"types": "./dist/index.d.ts",
			"import": "./dist/preview.js",
			"require": "./dist/preview.cjs"
		},
		"./preset": "./dist/preset.cjs",
		"./manager": "./dist/manager.js",
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"*.js",
		"*.d.ts"
	],
	"scripts": {
		"build": "tsup",
		"build-storybook": "storybook build",
		"dev": "vite",
		"lint": "eslint .",
		"preview": "vite preview",
		"prerelease": "zx scripts/prepublish-checks.js",
		"sb": "storybook dev -p 6006",
		"start": "run-p build:watch \"storybook --quiet\"",
		"test": "vitest run",
		"w": "vitest"
	},
	"eslintConfig": {
		"extends": [
			"plugin:storybook/recommended"
		]
	},
	"dependencies": {
		"@vitest/browser": "^2.1.3",
		"@vitest/expect": "^2.1.3",
		"js-base64": "^3.7.7",
		"pathe": "^1.1.2",
		"pixelmatch": "^6.0.0",
		"pngjs": "^7.0.0",
		"type-plus": "8.0.0-beta.6"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.9.0",
		"@eslint/js": "^9.13.0",
		"@repobuddy/typescript": "^2.1.0",
		"@storybook/addon-essentials": "^8.3.6",
		"@storybook/addon-interactions": "^8.3.6",
		"@storybook/addon-links": "^8.3.6",
		"@storybook/blocks": "^8.3.6",
		"@storybook/csf": "^0.1.11",
		"@storybook/experimental-addon-test": "^8.3.6",
		"@storybook/react": "^8.3.6",
		"@storybook/react-vite": "^8.3.6",
		"@storybook/test": "^8.3.6",
		"@types/jest-image-snapshot": "^6.4.0",
		"@types/node": "^18.19.58",
		"@types/pixelmatch": "^5.2.6",
		"@types/pngjs": "^6.0.5",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.3",
		"auto": "^11.2.1",
		"boxen": "^7.1.1",
		"dedent": "^1.5.3",
		"eslint": "^9.13.0",
		"eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
		"eslint-plugin-react-refresh": "^0.4.13",
		"eslint-plugin-storybook": "^0.9.0",
		"globals": "^15.11.0",
		"jest-image-snapshot": "^6.4.0",
		"playwright": "^1.48.1",
		"prettier": "^3.3.3",
		"prompts": "^2.4.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"storybook": "0.0.0-pr-26654-sha-49d01553",
		"tsup": "^8.3.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.11.0",
		"vite": "^5.4.9",
		"vitest": "^2.1.3",
		"zx": "^7.2.3"
	},
	"peerDependencies": {
		"storybook": ">= 8.3.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"bundler": {
		"exportEntries": [
			"src/index.ts"
		],
		"previewEntries": [
			"src/preview.ts"
		]
	},
	"storybook": {
		"displayName": "Image Snapshot integration",
		"supportedFrameworks": [
			"vite"
		],
		"icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
	}
}
