{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"//#check": {},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", "esm/**", "*.tsbuildinfo"]
		},
		"clean": {
			"cache": false
		},
		"coverage": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".storybook", "*.ts"],
			"outputs": ["coverage/**", "__vis__/**"]
		},
		"nuke": {
			"cache": false,
			"dependsOn": ["clean"]
		},
		"sb:build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".storybook"],
			"outputs": ["storybook-static/**"]
		},
		"t": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".storybook", "*.ts"],
			"outputs": ["__vis__/**"]
		}
	}
}
